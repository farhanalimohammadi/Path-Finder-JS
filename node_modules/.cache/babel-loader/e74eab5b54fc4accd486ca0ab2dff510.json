Coordinates = [nodeTwo.row, nodeTwo.col]\n  let xChange = Math.abs(nodeOneCoordinates[0] - nodeTwoCoordinates[0]);\n  let yChange = Math.abs(nodeOneCoordinates[1] - nodeTwoCoordinates[1]);\n  return (xChange + yChange);\n}"],"mappings":";AAAA,SAASA,WAAW,QAAQ,cAAc;AAE1C,OAAO,SAASC,uBAAuBA,CAACC,IAAI,EAAEC,KAAK,EAAEC,MAAM,EAAEC,cAAc,EAAEC,IAAI,EAAEC,SAAS,EAAE;EAC5F;EACDJ,KAAK,CAACK,QAAQ,GAAG,CAAC;EAClBL,KAAK,CAACM,SAAS,GAAG,OAAO;EACzB,IAAGH,IAAI,IAAI,OAAO,EAAE;IACnBH,KAAK,CAACO,aAAa,GAAG,CAAC;EACxB;EACA;EACC,IAAIC,cAAc,GAAGX,WAAW,CAACE,IAAI,CAAC;EACtC,OAAOS,cAAc,CAACC,MAAM,EAAE;IAC9B,IAAIC,WAAW,GAAGC,WAAW,CAACH,cAAc,EAAEL,IAAI,CAAC;IACjD,OAAOO,WAAW,CAACE,MAAM,KAAK,MAAM,IAAIJ,cAAc,CAACC,MAAM,EAAE;MAC7DC,WAAW,GAAGC,WAAW,CAACH,cAAc,EAAEL,IAAI,CAAC;IACnD;IACE,IAAIO,WAAW,CAACL,QAAQ,KAAKQ,QAAQ,EAAE,OAAO,KAAK;IACnDX,cAAc,CAACY,IAAI,CAACJ,WAAW,CAAC;IAClCA,WAAW,CAACE,MAAM,GAAG,SAAS;IAC9B;IACA,IAAIF,WAAW,CAACK,EAAE,KAAKd,MAAM,CAACc,EAAE,EAAE,OAAO,UAAU;IACnD;IACE,IAAIZ,IAAI,KAAK,KAAK,IAAIA,IAAI,KAAK,QAAQ,IAAIA,IAAI,IAAI,OAAO,EAAE;MAC1Da,eAAe,CAACjB,IAAI,EAAEW,WAAW,EAAEV,KAAK,EAAEC,MAAM,EAAEE,IAAI,EAAEC,SAAS,CAAC;IACpE,CAAC,MAAM,IAAID,IAAI,KAAK,UAAU,EAAE;MAC9Ba,eAAe,CAACjB,IAAI,EAAEW,WAAW,CAAC;IACpC;EACF;AACF;AAEA,SAASC,WAAWA,CAACH,cAAc,EAAEL,IAAI,EAAE;EACzC,IAAIc,cAAc,EAAEC,KAAK;EACzB,KAAK,IAAIC,CAAC,GAAG,CAAC,EAAEA,CAAC,GAAGX,cAAc,CAACC,MAAM,EAAEU,CAAC,EAAE,EAAE;IAChD,IAAGhB,IAAI,IAAI,OAAO,EAAE;MACnB,IAAI,CAACc,cAAc,IAAIA,cAAc,CAACV,aAAa,GAAGC,cAAc,CAACW,CAAC,CAAC,CAACZ,aAAa,EAAE;QACtFU,cAAc,GAAGT,cAAc,CAACW,CAAC,CAAC;QAClCD,KAAK,GAAGC,CAAC;MACV,CAAC,MAAM,IAAIF,cAAc,CAACV,aAAa,KAAKC,cAAc,CAACW,CAAC,CAAC,CAACZ,aAAa,EAAE;QAC5E,IAAIU,cAAc,CAACG,iBAAiB,GAAGZ,cAAc,CAACW,CAAC,CAAC,CAACC,iBAAiB,EAAE;UAC3EH,cAAc,GAAGT,cAAc,CAACW,CAAC,CAAC;UAClCD,KAAK,GAAGC,CAAC;QACV;MACD;IACD,CAAC,MAAM;MACN,IAAI,CAACF,cAAc,IAAIA,cAAc,CAACZ,QAAQ,GAAGG,cAAc,CAACW,CAAC,CAAC,CAACd,QAAQ,EAAE;QAC5EY,cAAc,GAAGT,cAAc,CAACW,CAAC,CAAC;QAClCD,KAAK,GAAGC,CAAC;MACV;IACD;EACA;EACAX,cAAc,CAACa,MAAM,CAACH,KAAK,EAAE,CA