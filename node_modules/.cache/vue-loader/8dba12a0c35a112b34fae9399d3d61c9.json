omponents/UI","sourcesContent":["<template>\n  <button\n    class=\"btn accent\"\n    :disabled=\"disabled\"\n    v-bind=\"$attrs\"\n    v-on=\"$listeners\"> <slot /> </button>\n</template>\n\n<script>\nexport default {\n  name: 'AppButton',\n  props: {\n    disabled: {\n      type: Boolean,\n      default: false\n    }\n  }\n}\n</script>\n\n\n<style lang=\"scss\">\n@import '@/scss/variables.scss';\n\n.btn {\n\tdisplay: flex;\n\talign-items: center;\n  color: rgb(255, 255, 255);\n\tborder: none;\n\tborder-radius: 3px;\n\theight: 40px;\n\tcursor: pointer;\n  transition: all 150ms ease-out;\n\t&:focus {\n\t\toutline: none;\n\t}\n\t&:disabled {\n\t\topacity: 0.5;\n\t}\n\n\timg {\n\t\twidth: 28px;\n\t\tmargin: 0 3px;\n\t\ttransition: all 150ms ease-out;\n\t}\n\tspan {\n\t\tmargin: 0 10px;\n\t}\n}\n\n.accent {\n\tbackground-color: $primary-low;\n\t&:hover:not(:disabled) {\n\t\tbackground-color: $primary;\n\t}\n}\n\n.danger {\n  background-color: $danger-low;\n\t&:hover:not(:disabled) {\n\t\tbackground-color: $danger;\n\t}\n\t&:disabled {\n\t\tbackground-color: $danger-low;\n\t}\n}\n\n.info {\n  background-color: $secondary-low;\n\t&:hover:not(:disabled) {\n\t\tbackground-color: $secondary;\n\t}\n}\n\n.warning {\n  background-color: $warning-low;\n\t&:hover:not(:disabled) {\n\t\tbackground-color: $warning;\n\t}\n}\n\n.white {\n  background-color: rgba(255, 255, 255, 0.65);\n\tcolor: #000000;\n\t&:hover:not(:disabled) {\n\t\tbackground-color: #ffffff;\n\t}\n}\n\n.hover {\n\tposition: absolute;\n\tright: 10px;\n\twidth: 55px;\n\theight: 50px;\n\tbackground-color: $dark-low;\n\t// border: solid 1px $dark-low;\n\toverflow: hidden;\n\ttransition: all 300ms ease-out;\n\t&:hover {\n\t\twidth: 170px;\n\t\tspan {\n\t\t\ttext-align: center;\n\t\t\topacity: 1;\n\t\t}\n\t}\n\timg {\n\t\twidth: 30px;\n\t\tmargin: 5px;\n\t}\n\tspan {\n\t\tposition: absolute;\n\t\topacity: 0;\n\t\ttop: 50%;\n\t\ttransform: translateY(-50%);\n\t\twidth: 100%;\n\t\ttransition: all 300ms ease-out;\n\t}\n}\n</style>\n"]}]}